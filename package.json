{
  "name": "acpl_rolland_exhibit",
  "version": "1.0.0",
  "description": "Exhibit Application for Rolland Center Kiosks",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "nodemonConfig": {
    "ignore": [
      "client/*"
    ],
    "delay": "100"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tr3ysmith/acpl_rolland_exhibit.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tr3ysmith/acpl_rolland_exhibit/issues"
  },
  "homepage": "https://github.com/tr3ysmith/acpl_rolland_exhibit#readme",
  "devDependencies": {
    "concurrently": "^6.0.0",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "is-empty": "^1.2.0",
    "socket.io": "^4.0.0",
    "winston": "^3.3.3"
  }
}
